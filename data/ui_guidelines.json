  "designPrinciples": [
    "Clean and modern aesthetic",
    "Data-driven visualizations",
    "Intuitive navigation",
    "Responsive design for all devices",
    "High contrast for readability",
    "Consistent spacing and typography",
    "Professional color scheme suitable for sports analytics",
    "Clear hierarchy and information architecture"
  ],
  "usageGuidelines": {
    "dashboard": {
      "layout": "Use a sidebar navigation with main content area",
      "cards": "Group related information in cards with consistent padding",
      "charts": "Use consistent colors and maintain readability",
      "tables": "Include sorting, filtering, and search capabilities"
    },
    "playerProfiles": {
      "layout": "Header with photo and basic info, followed by stats and attributes",
      "ratings": "Use progress bars or circular indicators for attribute ratings",
      "stats": "Group by categories (performance, physical, technical)"
    },
    "forms": {
      "validation": "Use clear error messages and success states",
      "layout": "Logical grouping of form fields",
      "buttons": "Primary action should be visually prominent"
    }
  }
}